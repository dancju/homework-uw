var x{1..12} >=0;
var y{1..10} >=0;
var z{1..8} >=0;
maximize obj:    +1.06*x[12]        +1.12**3*y[10]       +1.16**5*z[8];

s.t.  c1:  -x[1]              -y[1]                -z[1]               = -10000000;
s.t.  c2:  -x[2]  +1.06*x[1]  -y[2]                -z[2]               = 0;
s.t.  c3:  -x[3]  +1.06*x[2]  -y[3]                -z[3]               = 0;
s.t.  c4:  -x[4]  +1.06*x[3]  -y[4]  +1.12**3*y[1] -z[4]               = 0;
s.t.  c5:  -x[5]  +1.06*x[4]  -y[5]  +1.12**3*y[2] -z[5]               = 0;
s.t.  c6:  -x[6]  +1.06*x[5]  -y[6]  +1.12**3*y[3] -z[6] +1.16**5*z[1] = 0;
s.t.  c7:  -x[7]  +1.06*x[6]  -y[7]  +1.12**3*y[4] -z[7] +1.16**5*z[2] = 0;
s.t.  c8:  -x[8]  +1.06*x[7]  -y[8]  +1.12**3*y[5] -z[8] +1.16**5*z[3] = 0;
s.t.  c9:  -x[9]  +1.06*x[8]  -y[9]  +1.12**3*y[6]       +1.16**5*z[4] = 0;
s.t. c10: -x[10]  +1.06*x[9] -y[10]  +1.12**3*y[7]       +1.16**5*z[5] = 0;
s.t. c11: -x[11] +1.06*x[10]         +1.12**3*y[8]       +1.16**5*z[6] = 0;
s.t. c12: -x[12] +1.06*x[11]         +1.12**3*y[9]       +1.16**5*z[7] = 0;

s.t.  d1:  -x[1]  +y[1]       +3*z[1]                     <= 0;
s.t.  d2:  -x[2]  +y[2]       +3*z[2] +2*z[1]             <= 0;
s.t.  d3:  -x[3]  +y[3] -y[1] +3*z[3] +2*z[2] +z[1]       <= 0;
s.t.  d4:  -x[4]  +y[4] -y[2] +3*z[4] +2*z[3] +z[2]       <= 0;
s.t.  d5:  -x[5]  +y[5] -y[3] +3*z[5] +2*z[4] +z[3] -z[1] <= 0;
s.t.  d6:  -x[6]  +y[6] -y[4] +3*z[6] +2*z[5] +z[4] -z[2] <= 0;
s.t.  d7:  -x[7]  +y[7] -y[5] +3*z[7] +2*z[6] +z[5] -z[3] <= 0;
s.t.  d8:  -x[8]  +y[8] -y[6] +3*z[8] +2*z[7] +z[6] -z[4] <= 0;
s.t.  d9:  -x[9]  +y[9] -y[7]         +2*z[8] +z[7] -z[5] <= 0;
s.t. d10: -x[10] +y[10] -y[8]                 +z[8] -z[6] <= 0;

solve;
display x, y, z, obj;
end;
